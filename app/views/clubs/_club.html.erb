<div id="<%= dom_id club %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Active:</strong>
    <%= club.active? ? "Yes" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Owner:</strong>
    <%= club.owner_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= club.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= club.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Category:</strong>
    <%= club.category %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Level:</strong>
    <%= club.level %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Rules:</strong>
    <%= club.rules %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Public:</strong>
    <%= club.public? ? "Yes" : "No" %>
  </div>
</div>
